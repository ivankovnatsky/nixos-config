# K8s OpenWebUI NodePort forwarding (mini machine only)
:30081 {
	bind @bindAddress@
	reverse_proxy localhost:30081
}

# Jellyfin mini-vm forwarding (mini machine only)
:8096 {
	bind @bindAddress@
	reverse_proxy @miniVmIp@:8096
}

# Stash now runs directly on Mac mini via launchd daemon
# Port 9999 is handled by the stash service itself
